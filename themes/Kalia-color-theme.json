	"tokenColors": [
		{
			"scope": [
				"keyword.control.import",
				"keyword.control.from",
			],
			"settings": {
				"foreground": "#7c7c7c"
			}
		},
		{
			"scope": [
				"comment",
			],
			"settings": {
				"foreground": "#5c5c5c"
			}
		},
		{
			"scope": [
				"storage.type.class",
				"storage.type.function",
			],
			"settings": {
				"foreground": "#63addf"
			}
		},
		{
			"scope": [
				"entity.name.type.class",
				"entity.name.function"
			],
			"settings": {
				"foreground": "#4db8d3"
			}
		},
		// white
		{
			"scope": [
				"entity.name.type.module",
				"entity.name.tag",
				"entity.other.inherited-class",
				"punctuation.accessor",
				"meta.definition.variable",
				"meta.definition.property",
				"variable.other",
				"keyword.control.flow",
				"storage.modifier",
				"meta.function-call entity.name.function",
				"meta.definition.property entity.name.function",
				"meta.definition.method entity.name.function",
				"variable.language.this"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		// bold
		{
			"scope": [
				"entity.name.type.class",
				"entity.name.function",
				"storage.type.class",
				"storage.type.function"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		// italic
		{
			"scope": [
				"storage.type.class",
				"storage.type.function",
				"storage.modifier",
				"storage.type",
				"comment",
				"keyword.control.import",
				"keyword.control.from",
			],
			"settings": {
				"fontStyle": "italic",
			}
		},
		// strings
		{
			"scope": [
				"string.quoted.double",
				"string.quoted.single",
				"string.template"
			],
			"settings": {
				"foreground": "#b0cab2"
			}
		},
		// dim
		{
			"scope": [
				"meta.object.member variable.other.property",
				"variable.other.property"
			],
			"settings": {
				"foreground": "#ffffffBB"
			}
		},
		// ******************************** Markdown
		{
			"scope": [
				"markup.quote.markdown"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#a3a3a3"
			}
		},
		{
			"scope": [
				"markup.heading.markdown"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		// ******************************** json
		{
			"scope": [
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		// ******************************** typescript
		{
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#5c5c5c"
			}
		},
		{
			"scope": [
				"entity.name.type.interface",
				"meta.parameters meta.type.annotation entity.name.type",
				"meta.var-single-variable.expr meta.type.annotation entity.name.type",
			],
			"settings": {
				"foreground": "#8fecc5"
			}
		}
	]
}