	"tokenColors": [
		{
			"scope": [
				"keyword.control.import",
				"keyword.control.from",
			],
			"settings": {
				"foreground": "#7c7c7c"
			}
		},
		{
			"scope": [
				"comment",
			],
			"settings": {
				"foreground": "#5c5c5c"
			}
		},
		{
			"scope": [
				"storage.type.class",
				"storage.type.function",
			],
			"settings": {
				"foreground": "#8399a8"
			}
		},
		{
			"scope": [
				"entity.name.type.class",
				"entity.name.function"
			],
			"settings": {
				"foreground": "#4db8d3"
			}
		},
		// logic
		{
			"scope": [
				"keyword.control.conditional",
				"keyword.operator.relational",
				"keyword.control.loop",
				"keyword.control.flow"
			],
			"settings": {
				"foreground": "#b896bb"
			}
		},
		// white
		{
			"scope": [
				"entity.name.type.module",
				"entity.other.inherited-class",
				"punctuation.accessor",
				"meta.definition.variable",
				"meta.definition.property",
				"variable.other",
				"keyword.control.flow",
				"storage.modifier",
				"meta.function-call entity.name.function",
				"meta.definition.property entity.name.function",
				"meta.definition.method entity.name.function",
				"variable.language.this",
				"support.type.property-name.json"
			],
			"settings": {
				"foreground": "#cacaca"
			}
		},
		// bold
		{
			"scope": [
				"variable.other.constant",
				"entity.name.type.class",
				"entity.name.function",
				"storage.type.class",
				"meta.var.expr meta.brace.round",
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		// italic
		{
			"scope": [
				"storage.type",
				"storage.modifier",
				"comment",
				"keyword.control.import",
				"keyword.control.from",
			],
			"settings": {
				"fontStyle": "italic",
			}
		},
		// strings
		{
			"scope": [
				"string.quoted.double",
				"string.quoted.single",
				"string.template"
			],
			"settings": {
				"foreground": "#b0cab2"
			}
		},
		// dim
		{
			"scope": [
				"meta.object.member variable.other.property",
				"punctuation.terminator.statement",
				"variable.other.property",
				"storage.type",
			],
			"settings": {
				"foreground": "#c7c7c7BB"
			}
		},
		// ******************************** Markdown
		{
			"scope": [
				"markup.quote.markdown"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#a3a3a3"
			}
		},
		{
			"scope": [
				"markup.heading.markdown"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		// ******************************** typescript
		{
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#5c5c5c"
			}
		},
		{
			"scope": [
				"entity.name.type.interface",
				"meta.parameters meta.type.annotation entity.name.type",
				"meta.var-single-variable.expr meta.type.annotation entity.name.type",
			],
			"settings": {
				"foreground": "#8fecc5"
			}
		},
		// ******************************** html/react
		{
			"scope": [
				"entity.name.tag",
				"punctuation.definition.tag.begin",
				"punctuation.definition.tag.end"
			],
			"settings": {
				"foreground": "#eba3a3"
			}
		},
		// ******************************** css
		{
			"scope": [
				"meta.selector.css",
				"punctuation.section.property-list.begin.bracket.curly",
			],
			"settings": {
				"foreground": "#eba3a3"
			}
		},
		{
			"scope": [
				"support.type.property-name.css"
			],
			"settings": {
				"foreground": "#fff"
			}
		},
		{
			"scope": [
				"meta.property-value.css"
			],
			"settings": {
				"foreground": "#89a1b4"
			}
		}
	]
}