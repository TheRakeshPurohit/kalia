{
  "name": "kalia",
  "displayName": "Kalia",
  "description": "A thoughtful VS Code color theme",
  "publisher": "krasimir",
  "version": "2.1.0",
  "keywords": [
    "theme",
    "dark",
    "kalia"
  ],
  "icon": "icon.jpg",
  "repository": {
    "type": "git",
    "url": "https://github.com/krasimir/kalia"
  },
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Themes",
    "Languages"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:javascript",
    "onLanguage:javascriptreact"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "themes": [
      {
        "label": "Kalia",
        "uiTheme": "vs-dark",
        "path": "./themes/Kalia-color-theme.json"
      }
    ],
    "commands": [
      {
        "command": "Kalia.goto",
        "title": "Kalia goto"
      }
    ],
    "keybindings": [
      {
        "command": "Kalia.goto",
        "key": "ctrl+g ctrl+g",
        "mac": "cmd+g cmd+g",
        "when": "editorTextFocus"
      }
    ]
  },
  "galleryBanner": {
    "color": "#000000",
    "theme": "dark"
  },
  "scripts": {
    "build:client": "webpack --mode production --config webpack.config.client.js",
    "build:server": "webpack --mode production --config webpack.config.server.js",
    "release": "yarn build:client && yarn build:server && ./node_modules/.bin/vsce publish minor",
    "dev": "tsc -b -w",
    "test": "node ./__tests__/index.js"
  },
  "devDependencies": {
    "@types/lodash": "4.17.10",
    "@types/node": "22.7.6",
    "@types/vscode": "1.42.0",
    "code-inspector": "1.6.2",
    "lodash": "4.17.21",
    "pairify": "1.0.4",
    "ts-loader": "9.5.1",
    "typescript": "5.6.3",
    "vscode-languageclient": "9.0.1",
    "vscode-languageserver": "9.0.1",
    "webpack": "5.95.0",
    "webpack-cli": "5.1.4",
    "@vscode/vsce": "2.19.0"
  }
}
